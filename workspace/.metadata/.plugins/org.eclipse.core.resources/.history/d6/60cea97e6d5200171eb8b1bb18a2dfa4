package com.E;

 class Gr {
	
	private int age;
	
	public class Student {
		
		String name;

		public Student(){}
		
		public Student(String name,int a) {
			
			this.name = name;
			age  = a;
			
		}
		
		public void output(){
			System.out.println(this.name+""+age);
		}
	}
	
	public void output(){
		Student stu = new Student("¡ı —Û",24);
		stu.output();
	}
 }
 
 public class Group{
	public static void main(String[] args) {
		
		
		Gr g = new Gr();
		g.output();
		
		
		
		
	}
}


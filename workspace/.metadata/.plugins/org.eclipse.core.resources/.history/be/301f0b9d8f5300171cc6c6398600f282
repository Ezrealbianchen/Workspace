package com.Ezreal;

class Animal1 {
	private void beat() {
		System.out.println("心脏跳动");
	}
	
	public void breath() {
		this.beat();
		System.out.println("呼吸");
	}
	
}

class Wolf {
	private Animal1 a;
	private void run(){
		System.out.println("我在奔跑");
	}
	
	private void breath(){
		a.breath();
	}
	
}

class Bird1 {
	private Animal1 a;
	private void fly(){
		System.out.println("我在飞");
	}
	private void breath(){
		a.breath();
	}
}

public class TestConsist {
	public static void main(String[] args) {
		Bird1 b = new Bird1();
		b.breath();
	}
}
